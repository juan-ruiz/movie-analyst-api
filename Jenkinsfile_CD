pipeline {

   environment { 

        registry = "squiroz/apitest" 
        registryCredential = 'squiroz'
        dockerImage = '' 

    }
  agent  {label "principal"}
    
  stages {
        
    stage('docker pull') { 
        steps { 
        sh "docker pull squiroz/apitest" 
            }
          }  
    }
  }
